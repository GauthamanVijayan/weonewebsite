{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "npm run build",
        "outputDirectory": "weone/dist/browser" 
      }
    }
  ],
  "routes": [
    { "src": "/[^.]+", "dest": "/index.html" }, 
    { "src": "/", "dest": "/index.html" }      
  ]
}